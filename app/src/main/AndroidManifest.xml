<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zishen.linkedinhack">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".RecommendClothingActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ChooseStartTimeActivity"
            android:label="@string/title_activity_choose_time"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".ChooseLocationActivity"
            android:label="@string/title_activity_choose_location"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ClothingHabitQuizActivity"
            android:label="@string/title_activity_clothing_habit_quiz"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChooseEndTimeActivity"
            android:label="@string/title_activity_choose_end_time"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Quiz1"
            android:label="@string/title_activity_quiz1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Quiz2"
            android:label="@string/title_activity_quiz2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Quiz3"
            android:label="@string/title_activity_quiz3"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>