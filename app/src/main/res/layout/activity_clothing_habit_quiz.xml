        app:layout_anchor="@+id/include4"
        app:layout_anchorGravity="bottom|center_horizontal"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="503dp" />

</android.support.design.widget.CoordinatorLayout>
